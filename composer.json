{
    "name": "bondarde/laravel-toolbox",
    "description": "Collection of Laravel components and utilities",
    "type": "library",
    "authors": [
        {
            "name": "Alexander Bondar",
            "email": "alexander@bondar.de"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "laravel/framework": "^8.0|^9.0",
        "bondarde/php-utils": "^0.5.0",
        "laravel/fortify": "^1.8",
        "tinymce/tinymce": "^5.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "facade/ignition": "^2.5",
        "nunomaduro/collision": "^5.1|^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "BondarDe\\LaravelToolbox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "BondarDe\\LaravelToolbox\\LaravelToolboxServiceProvider"
            ]
        }
    }
}
