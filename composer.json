{
    "name": "bondarde/laravel-toolbox",
    "description": "Collection of Laravel components and utilities",
    "type": "library",
    "authors": [
        {
            "name": "Alexander Bondar",
            "email": "alexander@bondar.de"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "bondarde/php-utils": "^0.5.0",
        "jenssegers/agent": "^2.6",
        "laravel/fortify": "^1.10",
        "laravel/framework": "^9.0",
        "mateusjunges/laravel-acl": "^4.1",
        "tinymce/tinymce": "^5.10",
        "laravel/sanctum": "^2.14",
        "nunomaduro/collision": "^6.2",
        "spatie/laravel-ignition": "^1.3",
        "tweeb/tinymce-i18n": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "BondarDe\\LaravelToolbox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "BondarDe\\LaravelToolbox\\LaravelToolboxServiceProvider"
            ]
        }
    }
}
