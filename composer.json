{
    "name": "bondarde/lox",
    "description": "Collection of Laravel components and utilities",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Bondar",
            "email": "alexander@bondar.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "bondarde/php-utils": "^0.5.0",
        "deployer/deployer": "^7.0",
        "jenssegers/agent": "^2.6",
        "laravel/fortify": "^1.10",
        "laravel/framework": "^10.0",
        "tinymce/tinymce": "^5.10",
        "laravel/sanctum": "^3.0",
        "nunomaduro/collision": "^7.0",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-permission": "^5.5",
        "tweeb/tinymce-i18n": "^2.0",
        "aws/aws-sdk-php": "^3.233",
        "laravel/socialite": "^5.5",
        "firebase/php-jwt": "^6.3",
        "livewire/livewire": "^3.0@beta",
        "laravel/scout": "^10.2",
        "teamtnt/laravel-scout-tntsearch-driver": "^13.2"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "suggest": {
        "doctrine/dbal": "Needed for DB stats"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "BondarDe\\Lox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "BondarDe\\Lox\\LoxServiceProvider"
            ]
        }
    }
}
